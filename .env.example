# Configuration Discord
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_CLIENT_ID=your_discord_client_id_here
DISCORD_GUILD_ID=your_discord_guild_id_here

# Configuration Email (optionnel)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your_email@gmail.com
EMAIL_PASS=your_app_password_here

# Configuration Git
GIT_AUTO_COMMIT=true
GIT_AUTO_PUSH=false
GIT_REMOTE_URL=https://github.com/username/repository.git

# Configuration Base de données (optionnel)
DATABASE_URL=sqlite://./data/database.sqlite

# Configuration Développement
NODE_ENV=production
DEBUG=false
LOG_LEVEL=info

# Configuration Serveur (si applicable)
PORT=3000
HOST=localhost

# Configuration Sécurité
ENCRYPTION_KEY=your_32_character_encryption_key_here
JWT_SECRET=your_jwt_secret_here

# Configuration Alertes
ALERT_WEBHOOK_URL=your_webhook_url_for_alerts
ALERT_CHANNEL_ID=your_alert_channel_id

# Configuration Archivage
ARCHIVE_RETENTION_DAYS=365
AUTO_CLEANUP_ENABLED=true

# Configuration Personnalisation
DEFAULT_THEME=default
CUSTOM_THEMES_ENABLED=true